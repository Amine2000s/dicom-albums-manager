<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="mainPane" prefHeight="276.0" prefWidth="296.0" stylesheets="@Style/main-boardStyle.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chabiamin.dicomalbumsmanager.Controller.mainBoardController">
   <children>
      <Label prefHeight="62.0" prefWidth="303.0" styleClass="title" text="DICOM Album Manager" />
      <VBox layoutY="62.0" prefHeight="213.0" prefWidth="303.0" styleClass="sidebar">
         <children>
            <Button fx:id="metaDataQueryButton" mnemonicParsing="false" onAction="#showMetadataQuery" text="Metadata Query" />
            <Button fx:id="AlbumManagmentButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#showAlbumManagement" text="Album Managment" />
            <Button fx:id="settingsButton" layoutX="10.0" layoutY="36.0" mnemonicParsing="false" text="Settings" />
         </children>
      </VBox>
   </children>
</AnchorPane>
